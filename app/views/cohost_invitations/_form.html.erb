<%= form_for @cohost_invitation do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label :party_id %><br />
    <%= f.text_field :party_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :invite_name %><br />
    <%= f.text_field :invite_name %>
  </p>
  <p>
    <%= f.label :invite_email %><br />
    <%= f.text_field :invite_email %>
  </p>
  <p>
    <%= f.label :invite_code %><br />
    <%= f.text_field :invite_code %>
  </p>
  <p>
    <%= f.label :agree %><br />
    <%= f.check_box :agree %>
  </p>
  <p><%= f.submit %></p>
<% end %>
